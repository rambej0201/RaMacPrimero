<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Thobu</servlet-name>
  	<display-name>Thobu</display-name>
  	<description></description>
  	<servlet-class>cam.rss.Thobu</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetThobu</servlet-name>
  	<display-name>GetThobu</display-name>
  	<description></description>
  	<servlet-class>com.rss.GetThobu</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Thobu</servlet-name>
  	<url-pattern>/Thobu</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetThobu</servlet-name>
  	<url-pattern>/getThobu</url-pattern>
  </servlet-mapping>
</web-app>
